{
  "name": "gyratififel",
  "version": "1.0.0",
  "description": "Gyratififel",
  "repository": "git@github.com:MaxRozh/gyratifel-client.git",
  "author": "Maxim Rozhaev",
  "license": "MIT",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start:dev": "webpack serve",
    "build": "tsc && webpack --progress --config webpack.prod.config.js",
    "prod:analyze": "ANALYZE=true yarn build:prod",
    "lint:js": "eslint 'src/**/*.{ts,tsx}' --quiet --cache",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint": "yarn lint:js && yarn lint:css",
    "test": "jest",
    "test:related": "jest -o --passWithNoTests",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router": "^5.2.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "@keyv/redis": "^2.1.2",
    "@sentry/browser": "^6.10.0",
    "date-fns": "^2.23.0",
    "immer": "^9.0.5",
    "isomorphic-fetch": "^3.0.0",
    "keyv": "^4.0.3",
    "react-lazyload": "^3.2.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.9",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.7",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/react-router": "^5.1.16",
    "@types/react-router-config": "^5.0.3",
    "@types/react-router-dom": "^5.1.8",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "css-loader": "^6.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-jsdoc": "^36.0.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "file-loader": "^6.2.0",
    "hoist-non-react-statics": "^3.3.2",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.1.0",
    "redux-devtools-extension": "^2.13.9",
    "source-map-loader": "^3.0.0",
    "ts-import-plugin": "^1.6.7",
    "ts-jest": "^27.0.4",
    "ts-loader": "^9.2.4",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "@babel/core": "^7.14.8",
    "@babel/helper-call-delegate": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-do-expressions": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.14.5",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@sentry/integrations": "^6.10.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-dotenv": "^1.6.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "compression": "^1.7.4",
    "dotenv": "^10.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.31.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-redux-saga": "^1.2.1",
    "stylelint": "^13.13.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "lint-staged": "^11.1.1",
    "pm2": "^5.1.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.3.2",
    "react-test-renderer": "^17.0.2",
    "redux-mock-store": "^1.5.4",
    "webpack": "^5.47.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-filter-warnings-plugin": "^1.2.1",
    "webpack-manifest-plugin": "^4.0.2",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "yarn prettier --write 'src/**/*.{ts,tsx}'",
      "yarn lint:js",
      "git add"
    ],
    "src/**/*.css": [
      "yarn prettier --write 'src/**/*.css'",
      "yarn lint:css",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test:related"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
