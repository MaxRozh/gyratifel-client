{
  "name": "gyratififel",
  "version": "0.5.0",
  "description": "Gyratififel",
  "repository": "git@github.com:MaxRozh/gyratifel-client.git",
  "author": "Maxim Rozhaev",
  "license": "MIT",
  "private": true,
  "main": "index.js",
  "scripts": {
    "start:prod": "NODE_ENV=production next start -p 8080",
    "start:dev": "NODE_ENV=development next dev -p 8080",
    "build:prod": "NODE_ENV=production next build",
    "build:dev": "NODE_ENV=development next build",
    "prod:analyze": "ANALYZE=true yarn build:prod",
    "update-release-info": "bash workers/update-release-info.sh",
    "write-bundle-sizes": "bash workers/analyze-bundles.sh",
    "update-release-tag": "bash workers/update-release-tag.sh",
    "format-with-lints": "prettier --write .",
    "lint:js": "eslint 'src/**/*.{ts,tsx,js}' --quiet --cache --color",
    "lint:css": "stylelint 'src/**/*.css'",
    "lint": "yarn lint:js && yarn lint:css",
    "check-types": "tsc --pretty --noemit",
    "test": "jest",
    "test:related": "jest -o --passWithNoTests",
    "test:watch": "jest --watch",
    "test:watchAll": "jest --watchAll",
    "test:cov": "jest --coverage",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.4.15",
    "@graphql-tools/load-files": "^6.4.0",
    "@graphql-tools/merge": "^8.1.2",
    "@graphql-tools/schema": "^8.2.0",
    "@headlessui/react": "^1.4.1",
    "@keyv/redis": "^2.1.2",
    "@sentry/nextjs": "^6.13.2",
    "apollo-server-micro": "^3.3.0",
    "classnames": "^2.3.1",
    "date-fns": "^2.24.0",
    "graphql": "^15.6.0",
    "immer": "^9.0.6",
    "keyv": "^4.0.3",
    "next": "^11.1.2",
    "next-i18next": "^8.8.0",
    "next-pwa": "^5.3.1",
    "phaser": "^3.55.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-lazyload": "^3.2.0",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/helper-call-delegate": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-do-expressions": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.15.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.14.5",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@next/bundle-analyzer": "^11.1.2",
    "@sentry/integrations": "^6.13.2",
    "@types/enzyme": "^3.10.9",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.10.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.18",
    "@types/styled-jsx": "^3.4.4",
    "@types/webpack-env": "^1.16.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "@svgr/webpack": "^5.5.0",
    "autoprefixer": "^10.3.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.2.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-inline-dotenv": "^1.6.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "compression": "^1.7.4",
    "dotenv": "^10.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "^11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsdoc": "^36.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-redux-saga": "^1.2.1",
    "eslint-webpack-plugin": "^3.0.1",
    "hoist-non-react-statics": "^3.3.2",
    "husky": "^7.0.2",
    "jest": "^27.2.3",
    "lint-staged": "^11.1.2",
    "mini-css-extract-plugin": "^2.3.0",
    "pm2": "^5.1.2",
    "postcss": "^8.3.8",
    "postcss-functions": "^4.0.2",
    "pre-commit": "^1.2.2",
    "prettier": "^2.4.1",
    "prompts": "^2.4.1",
    "react-test-renderer": "^17.0.2",
    "redux-devtools-extension": "^2.13.9",
    "stylelint": "^13.13.1",
    "stylelint-config-css-modules": "^2.2.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0",
    "tailwindcss": "^2.2.16",
    "ts-import-plugin": "^1.6.7",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.3",
    "webpack": "^5.55.1",
    "webpack-cli": "^4.8.0",
    "webpack-filter-warnings-plugin": "^1.2.1",
    "webpack-manifest-plugin": "^4.0.2"
  },
  "engines": {
    "node": ">=12.22.4"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "yarn prettier --write 'src/**/*.{ts,tsx}'",
      "yarn lint:js",
      "git add"
    ],
    "src/**/*.css": [
      "yarn prettier --write 'src/**/*.css'",
      "yarn lint:css",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test:related"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}